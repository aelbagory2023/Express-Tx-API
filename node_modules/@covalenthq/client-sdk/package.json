    "type": "git",
    "url": "git+https://github.com/covalenthq/covalent-api-sdk-ts.git"
  },
  "author": {
    "name": "covalenthq",
    "url": "https://covalenthq.com"
  },
  "bugs": {
    "url": "https://github.com/covalenthq/covalent-api-sdk-ts/issues"
  },
  "homepage": "https://github.com/covalenthq/covalent-api-sdk-ts#readme",
  "dependencies": {
    "big.js": "^6.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/big.js": "^6.2.2",
    "@types/jest": "^29.5.5",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.6.1",
    "p-limit": "^3.1.0",
    "prettier": "^3.3.2",
    "rollup": "^3.29.1",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "scripts": {
    "test": "jest",
    "build": "rollup -c rollup.config.js",
    "clean": "rm -rf dist",
    "start": "rollup -c",
    "lint": "eslint .",
    "pretty": "prettier . --write",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./cjs": "./cjs/index.js",
    "./esm": "./esm/index.js",
    "./types/cjs": "./types/cjs/index.d.ts",
    "./types/esm": "./types/esm/index.d.ts",
    "./package.json": "./package.json"
  },
  "types": "./dist/esm/index.d.ts"
}
